{
  "name": "@openworkers/adapter-static",
  "version": "0.1.4",
  "description": "Static site adapter for OpenWorkers",
  "keywords": [
    "adapter",
    "openworkers",
    "edge",
    "workers",
    "static",
    "hosting"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openworkers/adapter-static.git"
  },
  "license": "MIT",
  "type": "module",
  "bin": {
    "adapter-static": "./bin/cli.js"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "index.d.ts",
  "files": [
    "bin",
    "files",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "build": "esbuild src/worker.js --bundle --outfile=files/worker.js --external:ROUTES --format=esm",
    "prepublishOnly": "npm run build",
    "format": "prettier --write ."
  },
  "dependencies": {
    "esbuild": "^0.27.2",
    "mime": "^4.1.0"
  },
  "devDependencies": {
    "@openworkers/workers-types": "^0.1.7",
    "prettier": "^3.8.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  }
}
